{
  "name": "metacoin",
  "version": "3.0.0",
  "description": "MetaCoin smart contract example for Tronbox",
  "scripts": {
    "compile": "tronbox compile",
    "deploy": "tronbox migrate --network=$NETWORK",
    "deploy:reset": "bun run deploy --reset",
    "format:fix": "biome format --write",
    "format": "biome format",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "spell-check": "cspell '**/*.{ts,sol,md,json}'",
    "test": "tronbox test"
  },
  "keywords": [
    "tron",
    "tronbox",
    "contracts",
    "solidity"
  ],
  "originalAuthors": "truffle-box",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "5.4.0",
    "picocolors": "1.1.1",
    "tronbox": "^4.4.0",
    "tronweb": "^6.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "cspell": "^9.3.0"
  }
}
